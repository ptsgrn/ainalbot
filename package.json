{
  "name": "patsabot",
  "version": "2.2.0",
  "private": "true",
  "description": "NodeJS bot to go with Thai wikipedia",
  "exports": "./src/index.js",
  "type": "module",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write . --ignore-unknown",
    "setup": "echo \">> This process will add 'patsabot' as a global command.\" && npm install -g .",
    "postsetup": ">> Done! You can now run 'patsabot' to start the bot.",
    "start": "node ./src/patsabot/job.js",
    "build": "tsc",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ptsgrn/patsabot.git"
  },
  "author": "Patsagorn Y.",
  "maintainers": [
    {
      "name": "Patsagorn Y.",
      "email": "mpy@toolforge.org",
      "web": "https://w.wiki/JSB"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ptsgrn/patsabot/issues"
  },
  "homepage": "https://github.com/ptsgrn/patsabot#readme",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "bin": {
    "patsabot": "./src/patsabot/run.js"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.0",
    "axios": "^1.3.5",
    "axios-rate-limit": "^1.3.0",
    "chalk": "^5.2.0",
    "cron": "^2.3.0",
    "cronstrue": "^2.26.0",
    "express-status-monitor": "^1.3.4",
    "matrix-org-irc": "^1.5.0",
    "meow": "^11.0.0",
    "moment": "^2.29.4",
    "mwn": "^1.11.5",
    "mysql2": "^2.3.3",
    "winston": "^3.8.2",
    "winston-discord-transport": "^1.3.0",
    "winston-transport": "^4.5.0"
  },
  "optionalDependencies": {
    "body-parser": "^1.20.2",
    "express": "^4.18.1"
  },
  "devDependencies": {
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.17",
    "@types/express-status-monitor": "^1.3.0",
    "@types/jsonfile": "^6.1.1",
    "@types/luxon": "^3.3.0",
    "@types/node": "^18.15.11",
    "dotenv": "^16.0.3",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "prettier": "2.8.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
